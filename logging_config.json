{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "unified": {
            "format": "%(asctime)s.%(msecs)03dZ %(story_type_padded) %(levelname)-8s %(message)s",
            "datefmt": "%Y-%m-%dT%H:%M:%S"
        }
    },
    "handlers": {
        "fileHandlerMain": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "unified",
            "filename": "",
            "mode": "a",
            "encoding": "utf-8"
        },
        "fileHandlerAlt": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "unified",
            "filename": "",
            "mode": "a",
            "encoding": "utf-8"
        }
    },
    "loggers": {
        "": {
            "handlers": ["fileHandlerMain", "fileHandlerAlt"],
            "level": "INFO"
        }
    }
}
